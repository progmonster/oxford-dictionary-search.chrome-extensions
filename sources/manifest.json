{
	"name": "Oxford Dictionary Search",
	"description": "Searches selected words in the Online Oxford Dictionary",
	"version": "0.1",
	"permissions": [
		"contextMenus",
		"tabs"
	],
	"icons": {
		"128": "images/oxford128.png",
		"54": "images/oxford54.png",
		"16": "images/oxford16.png"
	},
	"background": {
		"scripts": ["app.js"]
	},
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["content.js"]
		}
	],
	"manifest_version": 2
}